{
  "extends": [
    "next/core-web-vitals",
    "plugin:import/warnings",
    "prettier"
  ],
  "plugins": ["unused-imports"],
  "rules": {
    "semi": ["error", "always"],
    "semi-spacing": [
      "error",
      {
        "after": true,
        "before": false
      }
    ],
    "semi-style": ["error", "last"],
    "comma-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "unused-imports/no-unused-imports": "error"
  }
}
